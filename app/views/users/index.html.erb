<div class="row">
  <div class="col-md-3">
  <%= render 'profile', user: @user %>
  <%= render 'books/new', book: @book %>
  </div>
  
  <div class="col-md-8 offset-md-1">
    <h1>Users</h1>

    <table class="table table-striped">
      <tr><th>image</th><th>neme</th><th></th></tr>
      <% @users.each do |user| %>
      <tr>
        <td></td>
        <td><%= user.name %></td>
        <td><%= link_to "show", user_path(user.id) %></td>
      </tr>
      <% end %>
      
    </table>
  </div>
</div>
